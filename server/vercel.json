{
  "version": 2,
  "builds": [
    {
      "src": "api/[[...path]].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/[[...path]].js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/[[...path]].js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
